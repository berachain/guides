# Thirdweb Configuration
# Get these from https://thirdweb.com/dashboard
THIRDWEB_CLIENT_ID=your_client_id_here
THIRDWEB_SECRET_KEY=your_secret_key_here

# Server Wallet
# Address of the wallet that will receive payments
SERVER_WALLET_ADDRESS=0xYourServerWalletAddress

# Network Configuration
# Use berachainBepolia for testnet (Chain ID: 80069)
# Use berachainMainnet for mainnet (Chain ID: 80094)
CHAIN_ID=80069

# Server Configuration
PORT=3000
