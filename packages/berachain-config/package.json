{
  "name": "@branch/berachain-config",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./viem": {
      "types": "./dist/viem.d.ts",
      "import": "./dist/viem.mjs",
      "require": "./dist/viem.js"
    },
    "./ethers": {
      "types": "./dist/ethers.d.ts",
      "import": "./dist/ethers.mjs",
      "require": "./dist/ethers.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "ethers": "^6.0.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@berachain/eslint-config": "workspace:*",
    "@berachain/typescript-config": "workspace:*",
    "@types/node": "^20.0.0",
    "ethers": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "viem": "^2.0.0"
  }
} 